{
  "name": "kormo-connect",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm install --no-frozen-lockfile && vite",
    "build": "pnpm install --no-frozen-lockfile && rm -rf node_modules/.vite-temp && tsc -b && BUILD_MODE=prod vite build",
    "build:dev": "pnpm install --no-frozen-lockfile && rm -rf node_modules/.vite-temp && tsc -b && vite build",
    "lint": "pnpm install --no-frozen-lockfile && eslint .",
    "preview": "pnpm install --no-frozen-lockfile && vite preview",
    "clean": "rm -rf node_modules .pnpm-store pnpm-lock.yaml && pnpm store prune",
    "analyze": "pnpm build && npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.4",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.78.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.364.0",
    "pnpm-store": "link:/tmp/pnpm-store",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "lightningcss": "^1.25.1",
    "postcss": "8.4.49",
    "tailwindcss": "v3.4.16",
    "terser": "^5.31.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vite-plugin-source-identifier": "1.1.2"
  }
}